concurrent = 1
check_interval = 0

[[runners]]
  name     = "$NAME"
  url      = "$GITLAB_URL"
  token    = "$GITLAB_TOKEN"
  executor = "docker"
  [runners.docker]
    tls_verify    = false
    image         = "registry.alfred.yavende.com/dev_ops/gitlab_worker:v0.1.0"
    privileged    = false
    disable_cache = false
    volumes       = [
      "/cache:/cache",
      "/usr/bin/docker:/usr/bin/docker",
      "/var/run/docker.sock:/var/run/docker.sock"
    ]
    shm_size      = 0
  [runners.cache]
