concurrent = 1
check_interval = 0

[[runners]]
  name     = "$NAME"
  url      = "$GITLAB_URL"
  token    = "$GITLAB_TOKEN"
  executor = "docker"
  [runners.docker]
    tls_verify    = false
    image         = "ruby:2.3"
    privileged    = false
    disable_cache = false
    volumes       = [
      "/cache",
      "/usr/bin/docker:/usr/bin/docker"
    ]
    shm_size      = 0
  [runners.cache]
